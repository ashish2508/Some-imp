{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue5fe",
          "foreground": "#f10e5d",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " {{ .Path }} ",
          "foreground": "#0ef1a2",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": "<#3239cd>git:(</>{{ .HEAD }}<#156eea>)</>",
          "foreground": "#d629d3",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " âœ—",
          "foreground": "#d32c31",
          "type": "status",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}

